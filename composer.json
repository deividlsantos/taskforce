{
  "name": "github.com/tc-sistemas",
  "description": "TC Sistemas Comercial",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Thiago L.R. Silva",
      "email": "prog@tcsistemas.com",
      "homepage": "https://www.tcsistemas.com",
      "role": "Developer"
    }
  ],
  "config": {
    "vendor-dir": "vendor",
    "allow-plugins": {
      "composer/installers": true,
      "robloach/component-installer": true,
      "oomphinc/composer-installers-extender": true
