    "files": [
      "Source/Functions/functions.php"
    ]
  },
  "require": {
    "php": "^8.0",
    "ext-pdo": "*",
    "coffeecode/datalayer": "^1.1",
    "league/plates": "3.*",
    "coffeecode/router": "^1.0",
    "fullcalendar/fullcalendar": "^3.10",
    "dompdf/dompdf": "3.0",
    "phpmailer/phpmailer": "^6.5",
    "composer/metadata-minifier": "^1.0"
  }
}